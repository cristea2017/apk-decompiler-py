          },
          {
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "baby_turtle"
                },
                {
                  "test": "in_water",
                  "subject": "other",
                  "operator": "!=",
                  "value": true
                }
              ]
            },
            "max_dist": 35
          }
        ],
        "must_see": true,
        "must_see_forget_duration": 17.0
      },
      "minecraft:physics": {
      }
